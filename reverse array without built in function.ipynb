    "#slicing method\n",
    "import numpy as np\n",
    "x=np.array([10,20,30,40,50,60])\n",
    "def reve(x):\n",
    "    return x[::-1]\n",
    "\n",
    "resul=reve(x)\n",
    "print(resul)"
   ]
