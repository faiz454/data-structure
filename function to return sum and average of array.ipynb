    "import numpy as np\n",
    "x=np.array([10,20,30,40,50,60])\n",
    "def average(x):\n",
    "    l=len(x)\n",
    "    s=np.sum(x)\n",
    "    avg=s/l\n",
    "    return avg\n",
    "\n",
    "res=average(x)\n",
    "print(res)"
